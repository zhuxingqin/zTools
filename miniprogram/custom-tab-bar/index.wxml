<view class="cu-bar tabbar bg-white shadow">
  <view class="action flex center" wx:for="{{list}}" wx:key="index" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab">
    <view class="cuIcon-{{selected === index ? item.selectedIconPath : item.iconPath}}"></view>
    <view>{{item.text}}</view>
  </view>
</view>